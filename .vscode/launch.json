{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "AutoPub",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "GO_ENV": "development" // 如果有环境变量需要设置，可以在这里添加
      },
      "showLog": true,
      //   "preLaunchTask": "build", // 如果你有预编译任务（如task.json中的构建任务），使用后会导致只有热更新没法断点。
      "trace": "verbose"
    }
  ]
}
